# frkr-example-api configuration

# API Port
API_PORT=3000
FORWARD_PORT=3001

# Authentication Method (basic | oidc)
AUTH_METHOD=basic

# Basic Auth Credentials
FRKR_USERNAME=testuser
FRKR_PASSWORD=testpass

